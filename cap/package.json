{
  "name": "cap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "compile": "tsc -b configs/tsconfig.json",
    "lint": "eslint -c configs/eslint.config.js --cache --cache-location configs/.eslintcache",
    "format": "prettier . --write --cache --cache-location configs/.prettier-cache",
    "all": "pnpm compile && pnpm lint && pnpm format",
    "debug": "pnpm compile --verbose && pnpm lint --debug && pnpm format",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.8.1",
    "@emotion/react": "^11.14.0",
    "@xyflow/react": "^12.4.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.8.1",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
